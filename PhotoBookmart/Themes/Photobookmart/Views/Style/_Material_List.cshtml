@model List<ProductCategoryMaterial>
@{
    Layout = null;
}

@if (Model.Count != 0)
{
    <h4><strong>Cover Material</strong></h4>
}

@foreach (var e in Model ?? Enumerable.Empty<ProductCategoryMaterial>())
{
    <table width="1" border="0" cellspacing="0" cellpadding="3">
        <tbody>
            <tr>
                <td colspan="6">
                    <h4>@e.Name</h4>
                </td>
            </tr>
            @Html.Action("MaterialDetail", "Style", new { materialId = e.Id })
        </tbody>
    </table>
}